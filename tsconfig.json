{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "useUnknownInCatchVariables": false
  },
    "include": [
      "./scripts",
      "./test"
    ],
    "files": [
      "./hardhat.config.ts"
    ]
}
